{
  "name": "language-power-backend",
  "version": "1.2.1",
  "description": "",
  "main": "index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=8.10.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "dev": "nodemon app.js",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "test": "jest",
    "test:watch": "jest --watch --silent",
    "eslint": "eslint --debug ./",
    "format-fix": "yarn eslint --fix",
    "coverage": "yarn jest --collect-coverage",
    "ready": "yarn format-fix && yarn coverage",
    "analyze": "clinic doctor --autocannon [ / ] -- node app.js",
    "analyze:login": "clinic doctor --autocannon [ -m POST /api/user/login -i my-data.json ] -- node app.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autocannon": "^5.0.1",
    "eslint": "^7.27.0",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "prettier": "^2.3.0",
    "uuid": "^3.4.0"
  },
  "dependencies": {
    "@sentry/node": "5.17.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.11.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.12",
